{
  "name": "midway-example",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "dependencies": {
    "@midwayjs/bootstrap": "^2.14.0",
    "@midwayjs/core": "^2.14.0",
    "@midwayjs/decorator": "^2.14.0",
    "@midwayjs/logger": "^2.14.0",
    "@midwayjs/swagger": "^1.3.0",
    "@midwayjs/web": "^2.14.2",
    "@mw-components/jaeger": "^9.0.4",
    "@mw-components/koid": "^6.0.0",
    "@prisma/client": "^3.7.0",
    "@waiting/shared-core": "^15.1.0",
    "@waiting/shared-types": "^15.1.0",
    "bignumber.js": "^9.0.2",
    "chalk": "^5.0.0",
    "egg": "^2.33.1",
    "egg-scripts": "^2.15.2",
    "joi": "^17.5.0",
    "lodash": "^4.17.21",
    "prisma": "^3.7.0"
  },
  "devDependencies": {
    "@midwayjs/cli": "^1.2.93",
    "@midwayjs/egg-ts-helper": "^1.2.1",
    "@midwayjs/luckyeye": "^1.0.3",
    "@midwayjs/mock": "^2.14.0",
    "@types/jest": "^27.4.0",
    "@types/node": "17",
    "cross-env": "^7.0.3",
    "eslint": "^7.5.0",
    "jest": "^27.4.5",
    "mwts": "^1.3.0",
    "swagger-ui-dist": "^4.1.3",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "docker": "NODE_ENV=production pm2-runtime start ./bootstrap.js --name midway_prisma -i 1",
    "start": "egg-scripts start --daemon --title=midway_prisma --framework=@midwayjs/web",
    "stop": "egg-scripts stop --title=midway_prisma",
    "start_build": "npm run build && cross-env NODE_ENV=development midway-bin dev",
    "dev": "cross-env ets && cross-env NODE_ENV=local midway-bin dev --ts",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "midway-bin build -c",
    "check": "luckyeye",
    "prisma:pull": "npx prisma db pull --schema ./src/app/prisma/schema.prisma",
    "prisma:push": "npx prisma db push --schema ./src/app/prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema ./src/app/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema ./src/app/prisma/schema.prisma"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "midway-luckyeye": {
    "packages": [
      "midway_v2"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ddzyan/midway-prisma.git"
  },
  "author": "dingdongzhao",
  "license": "MIT"
}
